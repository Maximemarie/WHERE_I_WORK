<%= render "shared/headerlogo"%>
<div class = "container d-flex justify-content-between">
    <div class= "empty-image"></div>
    <%= image_tag "leane.jpeg", class:"photo"%>
    <div class= "empty-image"></div>
    <%# </div> %>

</div>
<div>
<%= link_to "My Reviews", reviews_path, class: "button1"%>
</div>


<h5> My Upcoming Reservation :</h5>
<div class= "place-info">
  <div class="cardforplace" style="background-image: url(<%= @upcoming_booking.place.photos.first.url %>)">

      <div class = "card-place-profile">
        <div class ="card-place-info">
          <div class= "card-place-name">
            <h6><%= @upcoming_booking.place.name %></h6>
          </div>
          </div>
        </div>
      </div>
      <div class = "card-place-timing">
      <%= @upcoming_booking.time.strftime("%A, %d %b %Y %l:%M %p") %>
      </div>

  </div>
</div>


<div>
  <h5>
    My Favorites :
  </h5>
  <% @favorites.each do |favorite| %>
  <%= favorite.place.name %>
  <% end %>
</div>
